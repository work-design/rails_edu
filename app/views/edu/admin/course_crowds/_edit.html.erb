<div class="ui modal" id="modal">
  <i class="close icon"></i>
  <div class="content">
    <%= form_with model: @course_crowd, url: admin_course_course_crowd_path(@course, @course_corwd) do |f| %>
      <%= render 'shared/error_messages', target: @course_crowd %>
