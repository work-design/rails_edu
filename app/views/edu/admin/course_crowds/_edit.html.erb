<div class="ui modal" id="modal">
  <i class="close icon"></i>
  <div class="content">
    <%= form_with model: @course_crowd, url: admin_course_course_crowd_path(@course, @course_crowd), method: :patch do |f| %>
      <%= render 'shared/error_messages', target: @course_crowd %>
      <%= f.select :teacher_id, options_for_select([[@course_crowd.teacher&.name, @course_crowd.teacher_id]], @course_crowd.teacher_id), { include_blank: true } %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
