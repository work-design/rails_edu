<%= render 'shared/error_messages', target: @lesson %>
<%= f.text_field :title %>
<%= f.file_field :videos, multiple: true %>
<%= render 'active_storage_ext/attachments/list_field', target: @lesson.videos %>
<%= f.file_field :documents, multiple: true %>
<%= render 'active_storage_ext/attachments/list_field', target: @lesson.documents %>
<%= f.collection_members_select :author_id, :id, :name %>
<%= f.submit %>
