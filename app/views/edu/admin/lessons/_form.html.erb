<%= form_with model: @lesson do |f| %>
  <%= render 'shared/error_messages', target: @lesson %>
  <%= f.text_field :title %>
  <%= f.file_field :videos, multiple: true %>
  <%= render 'active_storage/attachments/list_form', target: @course.video %>
  <%= f.file_field :documents, multiple: true %>
  <%= render 'active_storage/attachments/list_form', target: @course.document %>
  <%= f.submit %>
<% end %>
